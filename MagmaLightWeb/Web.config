            <add input="{HTTP_HOST}" pattern="^magmalight.com$" />
          </conditions>
          <action type="Redirect" url="http://www.naplampa.hu/{R:0}" />
        </rule>
        <rule name="Redirect magmalights.com to www.naplampa.hu" patternSyntax="ECMAScript" stopProcessing="true">
          <match url=".*" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="^magmalights.com$" />
          </conditions>
          <action type="Redirect" url="http://www.naplampa.hu/{R:0}" />
        </rule>
        <rule name="Redirect szellovas.hu to www.szellovas.hu" patternSyntax="ECMAScript" stopProcessing="false">
          <match url=".*" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="^szellovas.hu$" />
          </conditions>
          <action type="Redirect" url="http://www.szellovas.hu/{R:0}" />
        </rule>
        <rule name="Redirect www.szellovas.hu to www.naplampa.hu/SzelLovas" patternSyntax="ECMAScript" stopProcessing="true">
